{
  "name": "@dankreiger/with-http-reducer",
  "version": "1.1.18",
  "description": "Higher order reducer that adds standard async actions to a redux reducer",
  "keywords": [
    "async",
    "http",
    "reducers",
    "redux"
  ],
  "homepage": "https://github.com/dankreiger/with-http-reducer#readme",
  "bugs": {
    "url": "https://github.com/dankreiger/with-http-reducer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dankreiger/with-http-reducer.git"
  },
  "license": "ISC",
  "author": "Dan Kreiger <dan@dankreiger.com> (http://www.dankreiger.com/)",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "scripty",
    "build:watch": "scripty",
    "clean": "scripty",
    "sort": "scripty",
    "demo": "tar tvf $(npm pack)",
    "format": "scripty",
    "lint": "scripty",
    "prepare": "is-ci || husky install",
    "test": "jest --config jestconfig.json",
    "postinstall": "yarn sort"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css}": [
      "yarn lint",
      "prettier --single-quote --write"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "17.1.0",
    "@rollup/plugin-node-resolve": "11.2.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.0",
    "@semantic-release/npm": "7.0.10",
    "@types/deep-freeze": "0.1.2",
    "@types/jest": "26.0.20",
    "@types/rollup-plugin-size-snapshot": "^0.10.0",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "deep-freeze": "0.0.1",
    "eslint": "7.21.0",
    "husky": "5.1.3",
    "is-ci": "3.0.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "redux": "^4.1.0",
    "rimraf": "3.0.2",
    "rollup": "2.40.0",
    "rollup-plugin-size-snapshot": "0.12.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.30.0",
    "scripty": "2.0.0",
    "semantic-release": "17.4.1",
    "sort-package-json": "1.49.0",
    "ts-jest": "26.5.3",
    "typescript": "4.2.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ]
        }
      ]
    ]
  }
}
